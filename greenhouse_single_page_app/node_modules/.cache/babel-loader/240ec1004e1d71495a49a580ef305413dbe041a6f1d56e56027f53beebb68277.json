== 0) {\n      return 75; // Default to 75% if no expected pings\n    }\n\n    const rawPercentage = (pulled / expected) * 100;\n\n    let visualPercentage;\n\n    if (rawPercentage <= 75) {\n      visualPercentage = 75; // Clamp anything below 75% to 75% visual\n    } else {\n      // Rescale the range 75% - 100% to the visual range 75% (right) - 0% (left)\n      visualPercentage = 75 - ((rawPercentage - 75) * (75 / 25));\n    }\n\n    return visualPercentage;\n  },\n  markerThickness() {\n    return 5; // Or 3, depending on your preference\n  },\n},\n};\n</script>\n\n<style scoped>\n.ping-indicator {\n  position: relative;\n  display: flex;\n  width: 300px; /* Adjust as needed */\n  height: 30px;  /* Adjust as needed */\n  border: 0px solid #ccc;\n  overflow: hidden;\n}\n\n.segment {\n  height: 100%;\n  flex-shrink: 0;\n  position: absolute;\n}\n\n.segment-red {\n  background-color: red;\n  /* Visual range: Leftmost (100% actual) to where 85% actual falls */\n  left: 0;\n  width: calc((100 - 85) / 25 * 100%);\n}\n\n.segment-orange {\n  background-color: orange;\n  /* Visual range: Where 85% actual falls to where 95% actual falls */\n  left: calc((100 - 85) / 25 * 100%); /* Start after red */\n  width: calc((95 - 85) / 25 * 100%);\n}\n\n.segment-green {\n  background-color: green;\n  /* Visual range: Where 95% actual falls to rightmost (75% actual) */\n  left: calc((100 - 85) / 25 * 100% + (95 - 85) / 25 * 100%); /* Start after red + orange */\n  width: calc((85 - 75) / 25 * 100%);\n}\n\n.ping-marker {\n  position: absolute;\n  top: 0;\n  bottom: 0;\n  width: 5px; /* Or 3px */\n  background-color: black;\n  z-index: 10;\n  left: calc(${pingPercentage}% - ${markerThickness}px);\n}\n</style>"],"mappings":";;EACOA,KAAK,EAAC;AAAgB;;uBAA3BC,mBAAA,CAQM,OARNC,UAQM,G,0BAPJC,mBAAA,CAAuC;IAAlCH,KAAK,EAAC;EAAqB,6B,0BAChCG,mBAAA,CAA0C;IAArCH,KAAK,EAAC;EAAwB,6B,0BACnCG,mBAAA,CAAyC;IAApCH,KAAK,EAAC;EAAuB,6BAClCG,mBAAA,CAGG;IAFLH,KAAK,EAAC,aAAa;IAClBI,KAAK,EAPRC,eAAA;MAAAC,IAAA,U